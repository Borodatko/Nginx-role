server {
    listen 80;
    server_name www.{{ domain_name }};
    server_tokens off;

    error_log  /var/log/nginx/www.error.log;
    access_log /var/log/nginx/www.access.log;

    location / {
        proxy_pass http://www.upstream;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-Proto https;
    }
}
